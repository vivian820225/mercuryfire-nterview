{
  "routes": [{ "handle": "filesystem" }, { "src": "/.*", "dest": "/" }],
  "rewrites": [
    {
      "source": "/crudTest/:path*",
      "destination": "https://demo.mercuryfire.com.tw:49110/:path*"
    },
    {
      "source": "/crudTest/:path*/",
      "destination": "https://demo.mercuryfire.com.tw:49110/:path*/"
    }
  ]
}
